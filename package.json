{
  "name": "rabjs",
  "version": "8.0.0",
  "description": "rabjs is a JavaScript framework for low code",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*",
    "website"
  ],
  "resolutions": {
    "inversify": "5.0.1",
    "reflect-metadata": "0.1.13"
  },
  "scripts": {
    "i": "pnpm install",
    "dev": "pnpm -r run dev",
    "build": "pnpm -r run build",
    "clean": "pnpm -r run clean",
    "rm": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' +",
    "rmp": "find . -name \"package-lock.json\" -prune -exec rm -rf '{}' +",
    "build:website": "cd website && rm -rf .dumi dist && pnpm run build",
    "lint:es": "eslint --ext \"**/*.ts\" packages",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "update-version": "node scripts/update-version.js",
    "publish:packages": "pnpm -r publish --access public --no-git-checks --force",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ximing/greenjs.git"
  },
  "keywords": [],
  "author": "ximing",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ximing/rab/issues"
  },
  "homepage": "http://github.com/ximing/rab",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/generator": "^7.10.1",
    "@babel/parser": "^7.10.1",
    "@babel/plugin-external-helpers": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-proposal-do-expressions": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-syntax-decorators": "^7.10.1",
    "@babel/plugin-syntax-do-expressions": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.10.1",
