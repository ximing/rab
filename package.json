{
  "name": "rabjs",
  "version": "7.0.0",
  "description": "rabjs is a JavaScript framework for low code",
  "main": "index.js",
  "directories": {},
  "scripts": {
    "i": "PUPPETEER_DOWNLOAD_HOST=https://npm.taobao.org/mirrors SASS_BINARY_SITE=https://npm.taobao.org/mirrors/node-sass npm install --registry=https://registry.npm.taobao.org/",
    "dev": "lerna run dev",
    "init": "lerna bootstrap --force-local && lerna run bin",
    "rm": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' +",
    "rmp": "find . -name \"package-lock.json\" -prune -exec rm -rf '{}' +",
    "install:website": "cd website && npm i --registry=https://registry.npmjs.com",
    "build:website": "cd website && rm -rf .umi dist && npm run build",
    "lint:es": "eslint --ext \"**/*.ts\" packages",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "cs": "changeset",
    "cs:version": "changeset version",
    "cs:tag": "changeset tag",
    "prerelease": "pnpm build",
    "release": "changeset publish"
  },
