{
  "name": "@rabjs/observer",
  "version": "9.0.0",
  "description": "响应式状态库",
  "main": "lib/main.cjs",
  "module": "lib/main.js",
  "types": "lib/main.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/main.d.ts",
      "import": "./lib/main.js",
      "require": "./lib/main.cjs"
    }
  },
  "scripts": {
    "build": "tsx build.config.ts",
    "dev": "tsx build.config.ts --watch",
    "clean": "rm -rf lib build dist coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "tracer",
    "performance",
    "monitoring",
    "tracing"
  ],
  "author": "OSG Frontend Engineering",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "esbuild": "^0.19.12",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "ts-jest": "^29.0.0",
    "tsconfig": "workspace:*",
    "tsx": "^4.0.0",
    "typescript": "^5.1.3"
  },
  "files": [
    "lib/**/*",
    "build/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
