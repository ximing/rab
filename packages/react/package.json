{
  "name": "@rabjs/react",
  "version": "9.0.1",
  "description": "React 响应式状态库集成",
  "main": "lib/cjs/main.js",
  "module": "lib/esm/main.js",
  "types": "lib/esm/main.d.ts",
  "exports": {
    ".": {
      "types": "./lib/esm/main.d.ts",
      "import": "./lib/esm/main.js",
      "require": "./lib/cjs/main.js"
    }
  },
  "scripts": {
    "build": "rm -rf lib && tsc --project tsconfig.cjs.json && tsc --project tsconfig.esm.json",
    "dev": "tsc --project tsconfig.cjs.json --watch & tsc --project tsconfig.esm.json --watch",
    "clean": "rm -rf lib build dist coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "react",
    "reactive",
    "state",
    "hooks"
  ],
  "author": "ximing",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "esbuild": "^0.19.12",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-test-renderer": "^18.3.1",
    "ts-jest": "^29.0.0",
    "tsconfig": "workspace:*",
    "tsx": "^4.0.0",
    "typescript": "^5.1.3"
  },
  "files": [
    "lib/**/*",
    "build/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@rabjs/observer": "workspace:*",
    "@rabjs/service": "workspace:*",
    "nanoid": "^5.1.6",
    "use-sync-external-store": "^1.6.0"
  }
}
