{
    "name": "@rabjs/core",
    "version": "7.0.0",
    "description": "React and redux based framework.",
    "author": "ximing <ximing@meituan.com>",
    "homepage": "https://github.com/ximing/rab#readme",
    "license": "ISC",
    "main": "lib/index.js",
    "directories": {
        "lib": "old",
        "test": "__tests__"
    },
    "files": [
        "old",
        "lib"
    ],
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "dependencies": {
        "warning": "^4.0.3"
    },
    "peerDependencies": {
        "immer": "^6.0.2",
        "inversify": "^5.0.1",
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@types/jest": "^24.0.15",
        "@types/react": "^16.9.11",
        "@types/react-dom": "^16.9.3",
        "@types/react-test-renderer": "^16.9.1",
        "codecov": "^3.5.0",
        "immer": "^3.1.3",
        "inversify": "^5.0.1",
        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "react-test-renderer": "^16.11.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^2.6.3",
        "typescript": "^3.8.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ximing/rab.git"
    },
    "scripts": {
        "build": "yarn build:es5 & yarn build:esm & yarn build:next",
        "build:es5": "rimraf ./dist && tsc -p ./tsconfig.build.json",
        "build:esm": "rimraf ./esm && tsc -p ./tsconfig.build.json -m esnext --outDir esm",
        "build:next": "rimraf ./esnext && tsc -p ./tsconfig.build.json --target esnext --outDir esnext",
        "prettier": "prettier '@(src|demo)/**/*.@(ts|tsx|html|less)' --write",
        "lint": "yarn lint:eslint && yarn lint:tsc",
        "lint:eslint": "eslint . --ext .ts,.tsx --fix --max-warnings 0",
        "lint:tsc": "tsc -p ./tsconfig.json --noEmit",
        "test": "jest --collectCoverage"
    },
    "bugs": {
        "url": "https://github.com/ximing/rab/issues"
    }
}
